   "source": [
    "def climbStairs(n):\n",
    "    if n==1:\n",
    "        return 1\n",
    "\n",
    "    first = 1\n",
    "    second = 2\n",
    "\n",
    "    for i in range(2,n):\n",
    "        \n",
    "        third  = first + second \n",
    "        first = second\n",
    "        second = third\n",
    "\n",
    "    return second"
   ]
  },
