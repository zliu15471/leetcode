   "source": [
    "def mySqrt(x):\n",
    "    \n",
    "    if x <2:\n",
    "        return x\n",
    "\n",
    "\n",
    "    x0 = x\n",
    "    x1 = (x0 +x/x0) /2\n",
    "    while abs(x1-x0) >=1:\n",
    "        x0= x1\n",
    "        x1= (x0 + x/x0) /2\n",
    "\n",
    "    return int(x1)"
   ]
  },
